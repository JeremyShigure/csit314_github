<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button7"
        android:layout_width="413dp"
        android:layout_height="79dp"
        android:layout_marginBottom="4dp"
        android:background="#61636E"
        android:backgroundTint="#FFFFFF"
        android:text="LOGOUT"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        app:backgroundTint="#61636E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="397dp"
        android:layout_height="118dp"
        android:layout_marginTop="36dp"
        android:background="#FAF8F8"
        android:backgroundTint="#E6A08A"
        android:text="Update Prescription"
        android:textColor="#424242"
        android:textSize="16sp"
        app:backgroundTint="#C88787"
        app:icon="@drawable/ic_ups"
        app:iconSize="90dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button4"
        android:layout_width="397dp"
        android:layout_height="118dp"
        android:layout_marginTop="56dp"
        android:background="#FFFFFF"
        android:backgroundTint="#E6A08A"
        android:text="View Patient Prescription"
        android:textColor="#424242"
        android:textSize="16sp"
        app:backgroundTint="#9488AA"
        app:icon="@drawable/ic_vpp"
        app:iconSize="90dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="274dp"
        android:layout_height="35dp"
        android:layout_marginTop="152dp"
        android:fontFamily="@font/andada"
        android:gravity="center"
        android:text="Welcome to SaveYourLife"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="213dp"
        android:layout_height="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />
</androidx.constraintlayout.widget.ConstraintLayout>